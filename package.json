    "@types/koa-router": "^7.4.8",
    "@types/latinize": "^0.2.18",
    "@types/log": "^6.3.3",
    "@types/node": "^24.7.2",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^30.2.0",
    "jest-each": "^30.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "resolve-tspaths": "^0.8.23",
    "ts-jest": "^29.4.5",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
