  "type": "module",
  "scripts": {
    "build": "npx tsc && npx resolve-tspaths",
    "start": "npm run build && node build/src/main.js",
    "dev": "LOG_LEVEL=debug nodemon --inspect --watch src/ ./src/main.ts --exec \"npm run build && node --inspect build/src/main.js\" --verbose -e ts",
    "test": "npx jest",
    "test:watch": "npx jest --watchAll",
    "lint": "eslint . --ext .ts"
  },
  "engines": {
    "node": ">= 22"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/koa": "^2.15.0",
    "@types/koa-bodyparser": "^5.0.2",
    "@types/koa-json": "^2.0.23",
    "@types/koa-router": "^7.4.8",
    "@types/latinize": "^0.2.18",
    "@types/log": "^6.3.3",
    "@types/node": "^22.9.3",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "jest-each": "^29.7.0",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3",
    "resolve-tspaths": "^0.8.22",
    "ts-jest": "^29.2.5",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
